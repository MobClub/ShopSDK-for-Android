<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:paddingLeft="@dimen/shopsdk_default_refund_detail_content_padding_left_right"
	android:paddingRight="@dimen/shopsdk_default_refund_detail_content_padding_left_right"
	android:paddingBottom="10dp"
	android:orientation="vertical"
	android:background="@color/white"
	android:visibility="gone">
	<View
		android:layout_width="match_parent"
		android:layout_height="1dp"
		android:layout_marginBottom="10dp"
		android:background="#F7F7F7"/>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<View
			android:layout_width="0dp"
			android:layout_height="1dp"
			android:layout_weight="1"
			android:background="@color/transparent"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_modify_refund_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_modify_refund"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_fill_in_express_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_fill_in_express"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_modify_express_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_modify_express"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_query_express_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_query_express"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_reapply_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_reapply"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
		<TextView
			android:id="@+id/shopsdk_refund_detail_cancel_tv"
			android:layout_width="50dp"
			android:layout_height="20dp"
			android:layout_marginLeft="10dp"
			android:gravity="center"
			android:textColor="#999999"
			android:textSize="10sp"
			android:layout_alignParentLeft="true"
			android:text="@string/shopsdk_default_refund_detail_cancel"
			android:clickable="true"
			android:background="@drawable/shopsdk_default_corner_btn_grey_stroke"
			android:visibility="gone"/>
	</LinearLayout>
</LinearLayout>